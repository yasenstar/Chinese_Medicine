    <!-- http://www.semanticweb.org/yasen/ontologies/2024/3/cn-medo#FJ00020_小青龙汤 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/yasen/ontologies/2024/3/cn-medo#FJ00020_小青龙汤">
