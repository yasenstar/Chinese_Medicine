    <!-- http://www.semanticweb.org/yasen/ontologies/2024/3/cn-medo#FJ00013_调胃承气汤 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/yasen/ontologies/2024/3/cn-medo#FJ00013_调胃承气汤">
        <rdf:type rdf:resource="http://www.semanticweb.org/yasen/ontologies/2024/3/cn-medo#伤寒杂病论_01_辨太阳病脉证并治-上_方剂"/>
